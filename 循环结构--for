public class Circulate {
	public static void main(String[] args) {
	    int count=0;
	    for (int x =100; x <1000; x++) {
	    	int ge=x%10;
	    	int shi=x/10%10;
	    	int bai=x/10/10%10;
	    	if (x==ge*ge*ge+shi*shi*shi+bai*bai*bai){
	     		count++;
	    	}
	    }
	   System.out.print("æ°´ä»™èŠ±çš„ä¸ªæ•°ä¸ºï¼š"+count+"ä¸ª");//æ°´ä»™èŠ±çš„ä¸ªæ•°ä¸ºï¼š4ä¸ª
	}
}
  
