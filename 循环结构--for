/*æ­¤å¤„ä½¿ç”¨å¾ªç¯ä¸‹åŠ åˆ¤æ–­çš„æ‰‹æ³•ï¼Œè¿˜å¯ä»¥ç›´æ¥åœ¨å¾ªç¯ä¸­æ”¹æˆx+=2ä¸åˆ¤æ–­**/
public class Circulate {
	public static void main(String[] args) {
	    int sum = 0;
	    for (int x = 0; x <= 10; x++) {
	      if (x % 2 == 0) {
	        sum += x;
	      }
	    }
	    System.out.println("1-10ä¹‹é—´å¶æ•°å’Œä¸ºï¼š" + sum);//1-10ä¹‹é—´æ•°æ®ä¹‹å’Œä¸ºï¼š30
	}
